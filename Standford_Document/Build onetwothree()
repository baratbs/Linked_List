/* BUILD THE LIST {1,2,3} IN THE HEAP AND STORE ITS HEAD POINTER IN A LOCAL STACK VARJIABLE.
RETURN THE HEAD  POINTER TO THE CALLER./*


struct node* onetwothree(){

struct node * head=NULL;
struct node * second=NULL;
struct node * third=NULL;

head= malloc(sizeof(struct node));
second=malloc(sizeof(struct node));
third=malloc(sizeof(struct node));

head->data=1;
head->next=second;


second->data=2;
second->next=third;

third->data=3;
third->next=NULL;

return(head);
}
